<AnchorPane prefHeight="70.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BalController">
   <children>
      <TextField fx:id="balanceTF" layoutX="14.0" layoutY="31.0" prefHeight="25.0" prefWidth="103.0" />
      <Button fx:id="confirmBtn" layoutX="123.0" layoutY="30.0" mnemonicParsing="false" onAction="#confirm" text="Confirm Balance" />
      <Button fx:id="cancelBtn" layoutX="234.0" layoutY="30.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Label fx:id="addsetLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="6.0" prefHeight="17.0" prefWidth="261.0" style="-fx-alignment: center;" text="addsetLabel" wrapText="true" />
   </children>
</AnchorPane>
